<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <style name="ToolBarTextViewHelveticaNeue">
        <item name="android:textColor">@color/default_text</item>
        <!--<item name="android:fontFamily">@font/helveticaneue</item>-->
        <item name="android:textSize">@dimen/_15sdp</item>
    </style>

    <style name="ToolBarDefaultMediumTextView">
        <item name="android:textColor">@color/default_text</item>
        <item name="android:fontFamily">sans-serif-light</item>
        <item name="android:textAppearance">?android:attr/textAppearanceLarge</item>
    </style>

    <style name="Dialog" parent="Theme.AppCompat.Light.Dialog">
        <item name="colorAccent">@color/colorPrimaryDark</item>
    </style>

</resources>
